{
  "name": "@eko-ai/eko",
  "version": "4.0.4",
  "description": "Empowering language to transform human words into action.",
  "workspaces": [
    "packages/eko-core",
    "packages/eko-extension",
    "packages/eko-nodejs",
    "packages/eko-web"
  ],
  "scripts": {
    "build": "npx -y pnpm@latest -r --sequential build",
    "build-eko": "npx -y pnpm@latest --filter @eko-ai/eko build",
    "test": "npx -y pnpm@latest -r --sequential test",
    "clean": "rm -rf node_modules && npx -y pnpm@latest -r exec -- rm -rf node_modules dist"
  },
  "author": "FellouAI",
  "license": "MIT",
  "keywords": [
    "fellou",
    "eko"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FellouAI/eko.git"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}
